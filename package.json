{
  "name": "grpcity",
  "version": "1.3.0-beta.0",
  "description": "A powerful and complete gRPC framework for Node.js",
  "author": "Chakhsu.Lau",
  "license": "MIT",
  "keywords": [
    "grpcity",
    "grpc.js",
    "grpc",
    "microservice",
    "framework",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chakhsu/grpcity"
  },
  "homepage": "https://github.com/chakhsu/grpcity",
  "bugs": {
    "url": "https://github.com/chakhsu/grpcity/issues"
  },
  "main": "./types/index.js",
  "typings": "./types/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "mocha --exit --extension .test.js --recursive test",
    "build": "rm -rf types && tsc -P tsconfig.json",
    "lint:prettier": "prettier --cache --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "prettier": "pnpm lint:prettier --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,md,json,yaml}": "prettier --write"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.12",
    "@grpc/proto-loader": "^0.7.10",
    "debug": "^4.3.4",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "@tsconfig/node14": "^14.1.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.2",
    "chai": "^4.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "mocha": "^10.2.0",
    "prettier": "3.1.0",
    "snazzy": "^9.0.0",
    "typescript": "^5.3.2"
  }
}
